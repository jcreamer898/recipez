{
  "name": "recipez",
  "version": "0.2.0",
  "description": "Import recipes from cooking sites to your cookbook",
  "main": "./dist/index.ts",
  "bin": "./bin/recipez.js",
  "files": [
    "bin",
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/jcreamer898/recipez"
  },
  "keywords": [
    "recipe",
    "importer",
    "food",
    "recipes",
    "eating"
  ],
  "scripts": {
    "start": "./bin/recipez.js",
    "start:dev": "babel-node-ts ./src",
    "build": "babel -x .ts -d dist src",
    "dev": "npm run build -- --watch",
    "typecheck": "tsc --noEmit"
  },
  "author": "Jonathan Creamer",
  "license": "MIT",
  "dependencies": {
    "cheerio": "^1.0.0-rc.3",
    "colors": "^1.4.0",
    "marked": "^0.8.0",
    "meow": "^6.0.0",
    "mkdirp": "^1.0.3",
    "node-fetch": "^2.6.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.4",
    "@babel/node": "^7.8.4",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-typescript": "^7.8.3",
    "@types/cheerio": "^0.22.16",
    "@types/marked": "^0.7.2",
    "@types/node-fetch": "^2.5.4",
    "babel-node-ts": "^1.0.3",
    "typescript": "^3.7.5"
  }
}
